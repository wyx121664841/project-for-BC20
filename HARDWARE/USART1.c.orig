unsigned char CheckSum(u8 Buf[])
{
    u8 check;
    u8 i;
    check = 0;
    for(i = 0; i< Buf[9] +10; i++)
    {
        check += Buf[i];
    }
    Buf[Buf[9] +10] = check ;
    return check;
}
u8 checkbuf(u8 *buf)
{
    u16 reslt,j;
    u8 a;


    for(j=0; j<40; j++)
    {
        reslt+=buf[j];
    }
    a=reslt&0xff;
    return a;
}

